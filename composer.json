{
	"name": "doubleedesign/comet-plugin-blocks",
	"type": "wordpress-plugin",
	"description": "WordPress plugin implementing the Comet Components library and associated block editor customisations.",
	"version": "0.3.0",
	"homepage": "https://cometcomponents.io",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"autoload": {
		"psr-4": {
			"Doubleedesign\\Comet\\WordPress\\": "src/"
		}
	},
	"require": {
		"doubleedesign/comet-components-core": "0.3.0",
		"tomloprod/colority": "^1.4"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/doubleedesign/baguetteBox.js"
		}
	],
	"archive": {
		"exclude": [
			"src/*.scss",
			"src/*.css.map"
		]
	}
}

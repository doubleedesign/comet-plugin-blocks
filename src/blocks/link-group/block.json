{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "comet/link-group",
	"textdomain": "comet",
	"title": "Link group",
	"icon": "admin-links",
	"description": "List a collection of external or featured links",
	"category": "media",
	"acf": {
		"mode": "edit",
		"renderTemplate": "render.php"
	},
	"editorStyle": "file:./editor.css",
	"keywords": [
		"link",
		"links"
	],
	"attributes": {
		"colorTheme": {
			"type": "string",
			"default": "primary"
		}
	},
	"supports": {
		"mode": true,
		"jsx": true,
		"innerBlocks": false,
		"colorTheme": true
	}
}
